@{
    ViewBag.Title = "AddNewRole";
}

<h2>AddNewRole</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)


    <div>
        <div class="panel-default">
            <p>@Html.ActionLink("Create New Role", "AddNewRole")</p>
        </div>
        <pre>
<label>Roles</label>
@Html.DropDownList("roles","Select Role")<br />
<label>Users</label>
@Html.DropDownList("Membership", "Select User")
</pre>
        <input type="submit" value="Submit" />@ViewBag.FeedBack
    </div>
}
@if (Session["RoleFeedback"] != null || Session["RoleFeedback"] == null)
{
    @Session["RoleUserFeedback"]
}
@if (Session["RoleUserFeedback"] != null || Session["RoleUserFeedback"] == null)
{
    @Session["RoleFeedback"]
}

