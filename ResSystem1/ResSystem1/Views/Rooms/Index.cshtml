@model IEnumerable<Models.Rooms>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Residence.ResName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.beds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Residence.ResName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.beds)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roomType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.roomId }) |
            @Html.ActionLink("Details", "Details", new { id=item.roomId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.roomId })
        </td>
    </tr>
}

</table>
